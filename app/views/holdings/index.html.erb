<p id="notice"><%= notice %></p>

<h1>Holding</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th >Nombre</th>
      <th >Display</th>
      <th colspan="3">Acciones</th>
    </tr>
  </thead>

  <tbody>
    <% @holdings.each do |holding| %>
      <tr>
        <td><%= holding.id %></td>
        <td><%= holding.nombre_holding %></td>
        <td><%= holding.display_holding %></td>
        <td><%= link_to 'Mostrar', holding %></td>
        <td><%= link_to 'Editar', edit_holding_path(holding) %></td>
        <td><%= link_to 'Eliminar', holding, method: :delete, data: { confirm: 'Desea eliminar este registro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Holding', new_holding_path %>
